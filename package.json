{
  "name": "cheetahjs-svelte",
  "version": "1.0.1",
  "description": "CheetahJS components are built using Svelte & TailwindCSS",
  "main": "index.js",
  "author": {
    "name": "CheetahJS",
    "email": "git@cheetahjs.com"
  },
  "bugs": "https://github.com/cheetahjs-com/cheetahjs-svelte/issues",
  "homepage": "https://cheetahjs.com/",
  "license": "MIT",
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:unit": "vitest",
    "build:package": "svelte-kit sync && svelte-package",
    "package:publish": "standard-version && git push --follow-tags origin main && npm run build:package && npm publish ./package"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.2",
    "@sveltejs/kit": "^1.3.7",
    "@sveltejs/package": "1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "autoprefixer": "^10.4.13",
    "esbuild": "0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.3",
    "prettier-plugin-svelte": "^2.9.0",
    "prism-themes": "^1.9.0",
    "standard-version": "^9.5.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vitest": "^0.28.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheetahjs-com/cheetahjs-svelte"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "tailwindcss",
    "ui",
    "accordion",
    "alert",
    "avatar",
    "badge",
    "breadcrumb",
    "button-group",
    "button",
    "card",
    "carousel",
    "darkmode",
    "dropdown",
    "footer",
    "forms",
    "kbd",
    "list group",
    "mega menu",
    "modal",
    "navbar",
    "pagination",
    "progressbar",
    "rating",
    "sidebar",
    "skeleton",
    "spinner",
    "table",
    "tab",
    "timeline",
    "toast",
    "tooltip",
    "typography",
    "video"
  ],
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "type": "module"
}